<%- include('../_layouts/adminheader') -%>

<h2 class="page-title">Add Pages</h2>
<a href="/admin/pages" class="btn btn-primary">Back to All pages</a>
<form method="POST" class="mt-4" action="/admin/pages/add-page">
    <div class="form-group">
      <label for="title">Enter Title</label>
      <input type="text" class="form-control" placeholder="Enter Title" id="title" name="title">
    </div>
    <div class="form-group">
        <label for="title">Enter Slug</label>
        <input type="text" class="form-control" id="Slug" placeholder="Enter Slug" name="Slug">
      </div>
      <div class="form-group">
        <label for="title">Enter content</label>
        <textarea name="content" class="form-control" id="content" placeholder="Enter content" cols="30" rows="10"></textarea>
      </div>

    <button type="submit" class="btn btn-primary">Submit</button>
  </form>

<%- include('../_layouts/adminfooter') -%>
