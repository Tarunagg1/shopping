<%- include('_layouts/header') -%>
<!-- <%= product %> -->
<div class="row product">
    <% product.forEach(function(data){ %>
    <div class="col-sm-12 col-md-4">
        <a class="pa" href="">
            <img class="pimg" src="/product_img/<%= data.image %>" alt="notfoun" srcset="">
        </a>
        <h4><%= data.title %></h4>
        <h4><%= parseFloat(data.price).toFixed(2) %></h4>
        <a class="vd" href="/product/<%= data._id %>/>">View Details</a>
    </div>
    <% }) %>
</div>
</div>
</div>
<%- include('_layouts/footer') -%>